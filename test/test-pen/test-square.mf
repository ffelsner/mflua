mode_setup;
capsule_def(penbook) makepen((0,-1)--(3,-1)--(6,1)--(1,2)--cycle);
u#:=3/72.27pt#;
define_pixels(u);
%numeric u; u:=10;
z0=(0,0); z1=(50,0);
pickup pensquare xscaled 1u yscaled 1u ;
path p;
p := (0,0) -- (10,0) -- (10,10) -- (0,10) -- cycle;
%for n=1 step 1 until 1: draw (((0,0) -- (1,0) -- (1,1) -- (0,1) -- cycle) shifted (n,0)) scaled u; endfor ; 
