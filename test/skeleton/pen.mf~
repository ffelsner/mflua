mode_setup;
capsule_def(penbook) makepen((0,-1)--(3,-1)--(6,1)--(1,2)--cycle);
u#:=7/72.27pt#;
define_pixels(u);
%numeric u; u:=1;
z0=(0,0); z1=(50,0);
%pickup pensquare xscaled 5u yscaled 10u rotated 45;
%pickup penbook;
pickup pencircle xscaled 10u yscaled 5u rotated 45;
path p; 
p :=  (4.1,4){right} .. {right}(10,20) -- (30,20){right} .. {right}(40,0)  ; 
%%drawdot(4,4) scaled u;drawdot(40,0) scaled u;
%p :=  (0,0) ..(3,4) .. (6,2) .. (3,1) ..(1,4) ..(0,7) ;
draw p scaled u;
pickup pencircle scaled 0.1;
%unfill fullcircle xscaled 10u yscaled 5u rotated 45 shifted (4u,4u) ;
fill fullcircle xscaled 10u yscaled 5u rotated 45 shifted (4u,4u) ;
%drawdot (4u,4u);

%fill unitsquare scaled 1;
%%draw z0 .. controls (12.5,50) and (37.5,50) .. z1 .. (00,00) ;%-- z2 -- z3 -- cycle;
%fill z5 .. z4 ..z1 ..z3 ..z6 ..cycle;
%unfill 1/4[z2,z4] .. 3/4[z4,z2] .. cycle;
%unfill 1/4[z6,z2] .. 3/4[z6,z2] .. cycle;
shipit;
bye.
