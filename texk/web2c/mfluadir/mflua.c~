  if ( res==0 ){
      res = lua_pcall(L, 0, 0, 0);
      if (res==0){
	lua_getglobal(L, "POST_main_control");  /* function to be called */
	/* do the call (0 arguments, 1 result) */
	res = lua_pcall(L, 0, 1, 0) ;
	if (res==0) {
	  /* retrieve result */
	  int z = 0;
	  if (!lua_isnumber(L, -1)){
	    fprintf(stderr,"\n! Error:function `POST_main_control\n",lua_tostring(L, -1));
